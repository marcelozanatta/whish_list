<p id="notice"><%= notice %></p>

<h1>Product Personal Whish Lists</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Product</th>
      <th>Personal wish list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_personal_whish_lists.each do |product_personal_whish_list| %>
      <tr>
        <td><%= product_personal_whish_list.product %></td>
        <td><%= product_personal_whish_list.personal_wish_list %></td>
        <td><%= product_personal_whish_list.quantity %></td>
        <td><%= link_to 'Show', product_personal_whish_list %></td>
        <td><%= link_to 'Edit', edit_product_personal_whish_list_path(product_personal_whish_list) %></td>
        <td><%= link_to 'Destroy', product_personal_whish_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Personal Whish List', new_product_personal_whish_list_path %>
